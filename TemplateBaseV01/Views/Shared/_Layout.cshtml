<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/BootStrap/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/BootStrap/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/BootStrap/modernizr-2.6.2.js"></script>
</head>
<body>

    @{
        Html.RenderPartial("_HeaderNavBar");
    }
    <div class="container body-content">
        
        @RenderBody()
        
        
        @{
            Html.RenderPartial("_FooterNavBar");
        }

    </div>

    <script src="~/Scripts/BootStrap/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/BootStrap/bootstrap.min.js"></script>
</body>
</html>